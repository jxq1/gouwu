<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.FootprintDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.FootprintEntity" id="footprintMap">
        <result property="userid" column="userid"/>
        <result property="footid" column="footid"/>
        <result property="foottime" column="foottime"/>
        <result property="tablename" column="tablename"/>
        <result property="footname" column="footname"/>
        <result property="footpicture" column="footpicture"/>
    </resultMap>

    <select id="selectListVO" resultType="com.entity.vo.FootprintVO" >
        SELECT * FROM footprint  footprint
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <select id="selectVO" resultType="com.entity.vo.FootprintVO" >
        SELECT  footprint.* FROM footprint  footprint
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <select id="selectListView" resultType="com.entity.view.FootprintView" >
        SELECT  footprint.* FROM footprint  footprint
        <where> 1=1 ${ew.sqlSegment}</where>
    </select>

    <select id="selectView" resultType="com.entity.view.FootprintView" >
        SELECT * FROM footprint  footprint <where> 1=1 ${ew.sqlSegment}</where>
    </select>

</mapper>